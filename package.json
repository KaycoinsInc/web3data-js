{
  "name": "web3data-js",
  "version": "0.0.1",
  "description": "A javascript wrapper for accessing amberdata's public API.",
  "main": "index.js",
  "scripts": {
    "build": "parcel build  index.js --out-dir dist/node --out-file web3data.js --target node",
    "build-browser": "parcel build  browser.js --out-dir dist/browser --out-file web3data.min.js",
    "build-all": "npm run build; npm run build-browser",
    "lint": "eslint",
    "test": "nyc ava",
    "test-fast": "npm run test --fail-fast",
    "test-verbose": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3data/web3data-js.git"
  },
  "keywords": [
    "javascript",
    "blockchain",
    "web3"
  ],
  "author": "Taylor Dawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3data/web3data-js/issues"
  },
  "homepage": "https://github.com/web3data/web3data-js#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "nock": "^9.6.1",
    "nyc": "^13.0.1",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "got": "^9.2.1",
    "ky": "^0.2.0",
    "parcel": "^1.9.7"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "inherit": true
  }
}
