{
  "name": "web3data-js",
  "version": "0.1.0",
  "description": "A javascript wrapper for accessing amberdata's public API.",
  "main": "index.js",
  "scripts": {
    "build-node": "cross-env TARGET=NODE node scripts/build-script.js",
    "build-browser": "cross-env TARGET=BROWSER node scripts/build-script.js",
    "build": "cross-env TARGET=ALL node scripts/build-script.js",
    "lint": "eslint --ext .js --ignore-path .eslintignore .",
    "lintfix": "eslint --fix --ext .js --ignore-path .eslintignore .",
    "test-record": "cross-env AVA_PLAYBACK=record nyc -s ava",
    "test": "cross-env AVA_PLAYBACK=playback nyc -s ava",
    "test-fast": "cross-env AVA_PLAYBACK=playback nyc -s ava --fail-fast",
    "test-verbose": "cross-env AVA_PLAYBACK=playback nyc ava --verbose",
    "clean-fixtures": "rm -r test/fixtures"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3data/web3data-js.git"
  },
  "keywords": [
    "blockchain",
    "web3",
    "data"
  ],
  "author": "Taylor Dawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3data/web3data-js/issues"
  },
  "homepage": "https://github.com/web3data/web3data-js#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "ava-playback": "0.0.14",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.5",
    "dotenv": "^6.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "node-fetch": "^2.2.0",
    "nyc": "^13.0.1",
    "parcel": "^1.9.7",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "got": "^9.2.1",
    "ky": "^0.2.0"
  },
  "ava": {
    "files": [
      "test/web3data-browser.test.js"
    ],
    "require": [
      "babel-register",
      "ava-playback"
    ],
    "playbacks": "test/fixtures",
    "inherit": true
  }
}
