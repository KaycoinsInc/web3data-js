{
  "name": "web3data-js",
  "version": "0.0.1",
  "description": "A javascript wrapper for accessing amberdata's public API.",
  "main": "index.js",
  "scripts": {
    "build": "parcel build index.js",
    "lint": "echo \"Error: no linter specified\" && exit 1",
    "test": "ava",
    "test-fast": "ava --fail-fast",
    "test-verbose": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3data/web3data-js.git"
  },
  "keywords": [
    "javascript",
    "blockchain",
    "web3"
  ],
  "author": "Taylor Dawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3data/web3data-js/issues"
  },
  "homepage": "https://github.com/web3data/web3data-js#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "dotenv": "^6.0.0",
    "nock": "^9.6.1",
    "nyc": "^13.0.1",
    "parcel-bundler": "^1.9.7"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "got": "^9.2.1",
    "ky": "^0.2.0"
  },
  "ava": {
    "files": [
      "test/*.test.js"
    ],
    "require": [
      "babel-register"
    ],
    "inherit": true,
    "verbose": true
  }
}
