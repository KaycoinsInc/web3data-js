{
  "name": "web3data-js",
  "version": "0.3.6",
  "description": "A javascript wrapper for accessing amberdata's public API.",
  "main": "index.js",
  "scripts": {
    "build-test": "parcel ./examples/index.html -p 8080",
    "postinstall": "npm run build",
    "build": "npm run build-browser",
    "build-browser": "cross-env NODE_ENV=production rm dist/web3data.min.js; parcel build index.js --target browser --out-dir dist --out-file web3data.min.js",
    "build-node": "cross-env NODE_ENV=production rm dist/index.js; parcel build src/web3data.js --target node --out-dir dist --out-file index.js",
    "lint": "xo",
    "lintfix": "xo --fix",
    "test": "xo --quiet && cross-env AVA_PLAYBACK=playback nyc -s ava",
    "test-record": "cross-env AVA_PLAYBACK=record nyc -s ava",
    "test-live": "nyc -s ava",
    "test-fast": "cross-env AVA_PLAYBACK=playback nyc -s ava --fail-fast",
    "test-verbose": "cross-env AVA_PLAYBACK=playback nyc ava --verbose",
    "clean-fix": "rm -r test/fixtures",
    "example": "superstatic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3data/web3data-js.git"
  },
  "keywords": [
    "blockchain",
    "web3",
    "data"
  ],
  "author": "Taylor Dawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3data/web3data-js/issues"
  },
  "homepage": "https://github.com/web3data/web3data-js#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "ava": "^1.2.1",
    "browser-env": "^3.2.6",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "node-fetch": "^2.3.0",
    "nyc": "^13.3.0",
    "parcel": "^1.11.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "superstatic": "^6.0.4",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "ava-playback": "taylorjdawson/ava-playback",
    "axios": "^0.18.0",
    "capture-console": "^1.0.1",
    "isomorphic-ws": "^4.0.1",
    "ws": "^6.1.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "prettier": true
  }
}
