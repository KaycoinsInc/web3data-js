{
  "name": "web3data-js",
  "version": "0.3.6",
  "description": "A javascript wrapper for accessing amberdata's public API.",
  "main": "index.js",
  "scripts": {
    "build-node": "cross-env NODE_ENV=production TARGET=NODE node scripts/build-script.js",
    "build-browser": "cross-env NODE_ENV=production TARGET=BROWSER node scripts/build-script.js",
    "build": "cross-env NODE_ENV=production TARGET=ALL node scripts/build-script.js",
    "build-clean": "cross-env NODE_ENV=production TARGET=ALL node scripts/build-script.js clean",
    "lint": "xo",
    "lintfix": "xo --fix",
    "test-record": "cross-env AVA_PLAYBACK=record nyc -s ava",
    "test": "xo --quiet && cross-env AVA_PLAYBACK=playback nyc -s ava",
    "test-live": "nyc -s ava",
    "test-fast": "cross-env AVA_PLAYBACK=playback nyc -s ava --fail-fast",
    "test-verbose": "cross-env AVA_PLAYBACK=playback nyc ava --verbose",
    "example": "superstatic"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web3data/web3data-js.git"
  },
  "keywords": [
    "blockchain",
    "web3",
    "data"
  ],
  "author": "Taylor Dawson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/web3data/web3data-js/issues"
  },
  "homepage": "https://github.com/web3data/web3data-js#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "ava-playback": "0.0.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "node-fetch": "^2.2.0",
    "nyc": "^13.1.0",
    "parcel": "^1.10.3",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "superstatic": "^6.0.3",
    "xo": "^0.23.0"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "ava": {
    "files": [
      "test/**.test.js"
    ],
    "require": [
      "babel-register",
      "ava-playback"
    ],
    "playbacks": "test/fixtures",
    "inherit": true
  },
  "xo": {
    "semicolon": false,
    "space": true,
    "prettier": true
  }
}
